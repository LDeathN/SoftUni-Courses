# First Problem

energies = [int(x) for x in input().split()]
materials = [int(x) for x in input().split()]
toys = 0
count = 0
total_energy = 0
while energies and materials:
    count += 1
    elf = energies.pop(0)
    if elf < 5:
        count -= 1
        continue
    box = materials.pop()
    if count % 3 == 0 and count % 5 == 0:
        if elf >= box * 2:
            total_energy += box * 2
            elf -= box * 2
            energies.append(elf)
        else:
            elf *= 2
            energies.append(elf)
            materials.append(box)
    elif count % 3 == 0:
        if elf >= box * 2:
            total_energy += box * 2
            toys += 2
            elf = (elf - (box * 2)) + 1
            energies.append(elf)
        else:
            elf *= 2
            energies.append(elf)
            materials.append(box)
    elif count % 5 == 0:
        if elf >= box:
            total_energy += box
            elf -= box
            energies.append(elf)
        else:
            elf *= 2
            energies.append(elf)
            materials.append(box)
    else:
        if elf >= box:
            total_energy += box
            elf = (elf - box) + 1
            toys += 1
            energies.append(elf)
        else:
            elf *= 2
            energies.append(elf)
            materials.append(box)

print(f"Toys: {toys}")
print(f"Energy: {total_energy}")

if energies:
    print(f"Elves left: {', '.join([str(x) for x in energies])}")
if materials:
    print(f"Boxes left: {', '.join([str(x) for x in materials])}")



# Second Problem

rows, cols = [int(x) for x in input().split(", ")]
matrix = [[x for x in input().split(" ")] for _ in range(rows)]
decorations = []
gifts = []
cookies = []
position = None
collected_gifts = 0
collected_cookies = 0
collected_decorations = 0
stop = True

for i in range(rows):
    for j in range(cols):
        if matrix[i][j] == "Y":
            position = (i, j)
        elif matrix[i][j] == "D":
            decorations.append((i, j))
        elif matrix[i][j] == "G":
            gifts.append((i, j))
        elif matrix[i][j] == "C":
            cookies.append((i, j))

while stop:
    command = input()
    if command == "End":
        break
    command = command.split("-")
    direction, steps = command[0], int(command[1])

    if direction == "up":
        for i in range(steps):
            x, y = position
            if x - 1 >= 0:
                if (x - 1, y) in decorations:
                    collected_decorations += 1
                    decorations.remove((x - 1, y))
                if (x - 1, y) in cookies:
                    collected_cookies += 1
                    cookies.remove((x - 1, y))
                if (x - 1, y) in gifts:
                    collected_gifts += 1
                    gifts.remove((x - 1, y))
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[x][y] = "x"
                matrix[x - 1][y] = "Y"
                position = (x - 1, y)
                if not stop:
                    break
            else:
                position = (rows - 1, y)
                if position in decorations:
                    collected_decorations += 1
                    decorations.remove(position)
                if position in cookies:
                    collected_cookies += 1
                    cookies.remove(position)
                if position in gifts:
                    collected_gifts += 1
                    gifts.remove(position)
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[0][y] = "x"
                matrix[rows - 1][y] = "Y"
                if not stop:
                    break

    elif direction == "down":
        for i in range(steps):
            x, y = position
            if x + 1 < rows:
                if (x + 1, y) in decorations:
                    collected_decorations += 1
                    decorations.remove((x + 1, y))
                if (x + 1, y) in cookies:
                    collected_cookies += 1
                    cookies.remove((x + 1, y))
                if (x + 1, y) in gifts:
                    collected_gifts += 1
                    gifts.remove((x + 1, y))
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[x][y] = "x"
                matrix[x + 1][y] = "Y"
                position = (x + 1, y)
                if not stop:
                    break
            else:
                position = (0, y)
                if position in decorations:
                    collected_decorations += 1
                    decorations.remove(position)
                if position in cookies:
                    collected_cookies += 1
                    cookies.remove(position)
                if position in gifts:
                    collected_gifts += 1
                    gifts.remove(position)
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[rows - 1][y] = "x"
                matrix[0][y] = "Y"
                if not stop:
                    break

    elif direction == "right":
        for i in range(steps):
            x, y = position
            if y + 1 < cols:
                if (x, y + 1) in decorations:
                    collected_decorations += 1
                    decorations.remove((x, y + 1))
                if (x, y + 1) in cookies:
                    collected_cookies += 1
                    cookies.remove((x, y + 1))
                if (x, y + 1) in gifts:
                    collected_gifts += 1
                    gifts.remove((x, y + 1))
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[x][y] = "x"
                matrix[x][y + 1] = "Y"
                position = (x, y + 1)
                if not stop:
                    break
            else:
                position = (x, 0)
                if position in decorations:
                    collected_decorations += 1
                    decorations.remove(position)
                if position in cookies:
                    collected_cookies += 1
                    cookies.remove(position)
                if position in gifts:
                    collected_gifts += 1
                    gifts.remove(position)
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[x][cols - 1] = "x"
                matrix[x][0] = "Y"
                if not stop:
                    break

    elif direction == "left":
        for i in range(steps):
            x, y = position
            if y - 1 >= 0:
                if (x, y - 1) in decorations:
                    collected_decorations += 1
                    decorations.remove((x, y - 1))
                if (x, y - 1) in cookies:
                    collected_cookies += 1
                    cookies.remove((x, y - 1))
                if (x, y - 1) in gifts:
                    collected_gifts += 1
                    gifts.remove((x, y - 1))
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[x][y] = "x"
                matrix[x][y - 1] = "Y"
                position = (x, y - 1)
                if not stop:
                    break
            else:
                position = (x, cols - 1)
                if position in decorations:
                    collected_decorations += 1
                    decorations.remove(position)
                if position in cookies:
                    collected_cookies += 1
                    cookies.remove(position)
                if position in gifts:
                    collected_gifts += 1
                    gifts.remove(position)
                if not gifts and not cookies and not decorations:
                    print("Merry Christmas!")
                    stop = False

                matrix[x][0] = "x"
                matrix[x][cols - 1] = "Y"
                if not stop:
                    break

print(f"You've collected:")
print(f"- {collected_decorations} Christmas decorations")
print(f"- {collected_gifts} Gifts")
print(f"- {collected_cookies} Cookies")
for line in matrix:
    print(" ".join(line))
